string removeConsonants(string s){
        string S="";
        for(auto x:s)
        {
            if(x=='a' || x=='A'||x=='e' || x=='E'|| x=='i' || x=='I'|| 
                      x=='o' ||x=='O'||x=='u'|| x=='U')
            {
                S=S+x;
            }          
        }
        if(S.size()==0)
        {
            return "No Vowel";
        }
        return S;
    }
